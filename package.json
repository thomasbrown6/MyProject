    "firebase": "^5.3.1",
    "if-env": "^1.0.4",
    "mongoose": "^5.0.17",
    "mortgage-calculator-react": "^0.1.0",
    "node": "^10.8.0",
    "re-base": "^4.0.0",
    "react-big-calendar": "^0.19.2",
